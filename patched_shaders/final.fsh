varying vec2 texcoord;
varying vec4 color;

uniform sampler2D colortex3;	//taa mixed with everything

//Disable uneeded samplers and such if neither dof nor motionblur is used.


































    





	
	
	
	
	
	
	
	

	
	

	
		
	






	
	
	
		 
		 
		 
	
	
		 
		 
		 
		 

	
	

	
	
		
		
		
	






	
	
	
	
	
	
	



void main() {

	vec4 tex = texture2D(colortex3, texcoord.xy)*color;








	
		
	

	
		
	




	
    
    
	



	


	gl_FragData[0] = tex;
}


